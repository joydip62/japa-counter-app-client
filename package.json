{
  "name": "japa-count-app",
  "version": "1.0.12",
  "description": "this is a japa counter app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev:watch": "npx electronmon --trace-warnings .",
    "dist": "dotenv electron-builder"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "Jagannatha Patita-tarana",
  "license": "MIT",
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.japa.counter",
    "productName": "Japa-Counter",
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "asar": false,
    "files": [
      "main.js",
      "preload.js",
      "japa-app/build/**/*",
      "icon/**/*"
    ],
    "extraResources": [
      {
        "from": "icon",
        "to": "icon"
      }
    ],
    "win": {
      "icon": "icon/japa.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "joydip62",
        "repo": "japa-counter-app-client"
      }
    ],
    "protocols": {
      "name": "Japa Counter",
      "schemes": [
        "japa"
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron-log": "^5.4.1",
    "electron-updater": "^6.6.2",
    "xlsx": "^0.18.5"
  }
}
